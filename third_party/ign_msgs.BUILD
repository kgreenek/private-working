# Description:
#   Standard set of message definitions, used by Ignition Transport, and other applications.

licenses(["notice"])  # Apache 2.0

exports_files(["LICENSE"])

package(default_visibility = ["//visibility:public"])

load("@//build_tools/rules:glob_cc_hdr.bzl", "glob_cc_hdr")

# NOTE: This intentionally leaves out ignition's custom protobuf generator (Generator.cc/hh)
# because generating protos using ignition's non-standard method would mean losing the ability to
# use all the standard proto build rules, and standard protobuf tools.
cc_library(
    name = "ign_msgs",
    srcs = [
        "src/Factory.cc",
        # NOTE: Intentionally leaving out Generator.cc.
        #"src/Generator.cc",
        "src/ign.cc",
        "src/Utility.cc",
    ],
    hdrs = [
        ":config_hh",
        ":hdrs",
        ":message_types_hh",
        ":msgs_hh",
        ":msgs_msgs_hh",
    ],
    includes = [
        ".",
        "include",
    ],
    deps = [
        ":all_cc_proto",
        "//external:ign_math",
    ],
)

filegroup(
    name = "hdrs",
    srcs = glob(
        ["include/ignition/msgs/*.hh"],
        exclude = ["include/ignition/msgs/Generator.hh"],
    ),
    visibility = ["//visibility:private"],
)

genrule(
    name = "config_hh",
    outs = ["ignition/msgs/config.hh"],
    cmd = """
        echo '/* Config.hh. Generated by CMake for ignition-msgs. */' >> $@
        echo '' >> $@
        echo '/* Version number */' >> $@
        echo '#define IGNITION_MSGS_MAJOR_VERSION 0' >> $@
        echo '#define IGNITION_MSGS_MINOR_VERSION 7' >> $@
        echo '#define IGNITION_MSGS_PATCH_VERSION 0' >> $@
        echo '' >> $@
        echo '#define IGNITION_MSGS_VERSION "0.7"' >> $@
        echo '#define IGNITION_MSGS_VERSION_FULL "0.7.0"' >> $@
        echo '' >> $@
        echo '#define IGNITION_MSGS_VERSION_HEADER "Ignition msgs, version 0.7.0\\nCopyright (C) 2014 Open Source Robotics Foundation.\\nReleased under the Apache 2.0 License.\\n\\n"' >> $@
        echo '' >> $@
        echo '/* #undef BUILD_TYPE_PROFILE */' >> $@
        echo '/* #undef BUILD_TYPE_DEBUG */' >> $@
        echo '#define BUILD_TYPE_RELEASE 1' >> $@
    """,
    visibility = ["//visibility:private"],
)

glob_cc_hdr(
    name = "message_types_hh",
    targets = [":all_cc_proto"],
    prefix_lines = ["// Automatically generated"],
    out = "ignition/msgs/MessageTypes.hh",
    visibility = ["//visibility:private"],
)

genrule(
    name = "msgs_hh",
    srcs = [
        ":config_hh",
        ":message_types_hh",
        ":hdrs",
    ],
    outs = ["ignition/msgs.hh"],
    cmd = """
        echo '// Automatically generated' >> $@
        for src in $(SRCS); do
          echo '#include \"ignition/msgs/'"$$(basename $$src)"'\"' >> $@
        done
    """,
    visibility = ["//visibility:private"],
)

# For some reason, the ignition developers generate an exact copy of this msgs.hh file, and they
# install it at ignition/msgs.hh and ignition/msgs/msgs.hh. Why? No idea, but some files include one
# and some files include the other, so we have to generate both.
genrule(
    name = "msgs_msgs_hh",
    srcs = [":msgs_hh"],
    outs = ["ignition/msgs/msgs.hh"],
    cmd = "cp $< $@",
    visibility = ["//visibility:private"],
)

